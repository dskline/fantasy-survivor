{
  "name": "@frontendlive/fantasy-survivor",
  "version": "1.0.0",
  "scripts": {
    "start": "doppler run -- next dev",
    "build": "doppler run -- next build",
    "lint": "eslint features",
    "lint-fix": "eslint features --fix && prettier --write '{features,pages}/**/*' && eslint features --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage && coveralls < coverage/lcov.info",
    "codegen": "doppler run -- graphql-codegen --require dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@headlessui/react": "1.6.6",
    "@supabase/auth-helpers-react": "0.2.3",
    "@supabase/auth-helpers-nextjs": "0.2.7",
    "@supabase/supabase-js": "1.35.6",
    "@supabase/ui": "0.36.5",
    "classnames": "2.3.1",
    "framer-motion": "7.2.0",
    "graphql": "16.6.0",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-icons": "4.4.0",
    "react-toastify": "9.0.8",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@frontendlive/eslint-config": "1.1.2",
    "@graphql-codegen/cli": "2.11.6",
    "@graphql-codegen/near-operation-file-preset": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@testing-library/react": "13.3.0",
    "@types/node": "18.7.6",
    "@types/react": "18.0.17",
    "autoprefixer": "10.4.8",
    "c8": "7.12.0",
    "coveralls": "3.1.1",
    "dotenv": "16.0.1",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "jsdom": "20.0.0",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4",
    "vite": "3.0.9",
    "vitest": "0.22.1"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "**/*.js"
    ],
    "extends": [
      "@frontendlive",
      "next"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "settings": {
      "react": {
        "version": "detect"
      },
      "polyfills": [
        "fetch"
      ]
    }
  }
}
